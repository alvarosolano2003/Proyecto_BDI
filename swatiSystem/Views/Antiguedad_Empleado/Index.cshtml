@model IEnumerable<swatiSystem.Antiguedad_Empleado>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Concepto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Limite_Antiguedad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Porcentage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreadoEn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActualizadoEn)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Concepto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Limite_Antiguedad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Porcentage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreadoEn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ActualizadoEn)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
